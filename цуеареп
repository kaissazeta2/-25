#include <iostream>
#include <string>
#include <windows.h>
#include <algorithm>
#include <vector>
#include <cmath>
int main() {
	using namespace std;
	int n,m;
	cin >> n>>m;
	int a;
	vector<int> doma;
	vector<int> kolvo;
	for (int i = 0; i < n; i++) {
		cin >> a;
		doma.push_back(a);
		vector<int>::iterator k = find(doma.begin(), doma.end(), a);
		if (k != doma.end()) {
			kolvo[*k] + 1;
		}
		else {
			kolvo.push_back(1);
		}
	}
	for (int i = 1; i <= m; i++) {
		if (kolvo[*find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end()))] < kolvo[*find(doma.begin(), doma.end(), *max_element(doma.begin(), doma.end()))]) {
			if (kolvo[*find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end()))] >= m - i) {
				i += kolvo[*find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end()))];
				if (find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end())+1) != doma.end()) {
					kolvo[*find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end()) + 1)] += kolvo[*find(doma.begin(), doma.end(), *min_element(doma.begin(), doma.end()))];
				}
			}
			else { break; }
		}
	}
	return 0;
}
